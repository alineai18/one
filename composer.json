{
    "name": "aplus/one",
    "type": "project",
    "description": "Aplus Framework One Project",
    "keywords": [
        "one",
        "micro",
        "microservice"
    ],
    "homepage": "https://docs.aplus-framework.com/guides/projects/one/",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "aplus/framework": "dev-master"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.10",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "natanfelles/php-server": "^2.11",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\One\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/projects/one/issues",
        "wiki": "https://gitlab.com/aplus-framework/projects/one/wikis/home",
        "source": "https://gitlab.com/aplus-framework/projects/one",
        "docs": "https://docs.aplus-framework.com/guides/projects/one/"
    }
}
